@model IEnumerable<ABCRetailWebApp.Models.BlobModel>

@{
    ViewData["Title"] = "Blob List";
    var containerName = ViewData["ContainerName"]?.ToString();
}

<div class="container blob-list-container">
    <h1 class="page-title"> Media List </h1>
    <h2>Upload media here</h2>

    <!-- Navigation for container selection -->
    <div class="btn-group mb-4" role="group">
        <a asp-action="MediaContent" asp-route-containerName="media-content" class="btn btn-primary">View Content</a>
    </div>
</div>
