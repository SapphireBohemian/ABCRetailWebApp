@{
    ViewData["Title"] = "File Management";
}

<div class="container text-center">
    <h1 class="mb-4">File Management</h1>
    <p class="lead mb-4">Easily upload and manage your files with the options below.</p>

    <div class="btn-group mb-4">
        <a asp-action="Upload" class="btn btn-primary btn-lg">Upload File</a>
        <a asp-action="List" asp-route-directoryName="files" class="btn btn-secondary btn-lg">View File List</a>
    </div>
</div>
